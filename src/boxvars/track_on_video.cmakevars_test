#set(CAMERA_CREDENTIALS  "physycom:ohnounapassWORD55")
set(CAMERA_CREDENTIALS   root:camera )
set(FILENAME             track.avi)

set(DETECTION_TYPE_TRACK tracking )
set(BARRIER_TOP          0 )
set(BARRIER_BOTTOM       1080 )
set(BARRIER_LEFT         700 )
set(BARRIER_RIGHT        1150 )
set(TOLL                 100 )
set(SCALE_X              1.0f )
set(SCALE_Y              10.0f )

set(BARRIER_IN           LEFT )
set(BARRIER_OUT          RIGHT )

set(PP                   0.0625 ) # avg 1 / ( number of frames needed to cross the ROI in to out )
set(PM                   0.0625 ) # avg 1 / ( number of frames needed to cross the ROI out to in )
set(DIRECTION            LEFT_RIGHT )
